<h1>Wikis#index</h1>
<p>Find me in app/views/wikis/index.html.erb</p>
<h1>My Wikis</h1>

<div class="row">
  <div class="col-md-8">
    <% @wiki.each do |wiki| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to wiki.title, wiki %>
          </h4>
          
        </div>
      </div>
    <% end %>
  </div>
  <div class="col-md-4">
    <%= link_to "New Public Wiki", new_wiki_path, class: 'btn btn-success' %>
  </div>
</div>